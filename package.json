{
  "name": "los-angeles-mikvah-society",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build-test": "npm run copy-test && npm run env -s && ng build --configuration=test --subresource-integrity",
    "build-prod": "npm run copy-prod && npm run env -s && ng build --prod --subresource-integrity",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js",
    "lint": "ng lint --type-check && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "ng test",
    "test:ci": "npm run lint -s && ng test --single-run --code-coverage",
    "e2e": "ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "generate": "ng generate",
    "copy-test": "cp src/_redirects-test src/_redirects",
    "copy-prod": "cp src/_redirects-prod src/_redirects"
  },
  "dependencies": {
    "@angular/animations": "^10.2.5",
    "@angular/cdk": "^10.1.1",
    "@angular/common": "^10.2.5",
    "@angular/compiler": "^10.2.5",
    "@angular/core": "^11.0.5",
    "@angular/forms": "^10.2.5",
    "@angular/localize": "^10.2.5",
    "@angular/material": "^10.1.1",
    "@angular/platform-browser": "^10.2.5",
    "@angular/platform-browser-dynamic": "^10.2.5",
    "@angular/platform-server": "^10.2.5",
    "@angular/router": "^10.2.5",
    "@angular/service-worker": "^10.2.5",
    "@auth0/angular-jwt": "^4.2.0",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "@types/auth0-js": "^9.14.6",
    "angulartics2": "^8.3.0",
    "auth0-js": "^9.19.0",
    "bootstrap": "4.5.0",
    "core-js": "^3.22.7",
    "font-awesome": "^4.7.0",
    "libphonenumber-js": "^1.10.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.2",
    "rollbar": "^2.25.0",
    "rxjs": "6.5.4",
    "tslib": "^2.4.0",
    "typed-countries": "^1.1.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.7",
    "@angular/cli": "^10.2.4",
    "@angular/compiler-cli": "^10.2.5",
    "@angular/language-service": "^10.2.5",
    "@biesbjerg/ngx-translate-extract": "^4.2.0",
    "@ngx-rocket/scripts": "^4.0.0",
    "@types/jasmine": "^3.10.6",
    "@types/jasminewd2": "^2.0.10",
    "@types/lodash": "^4.14.182",
    "@types/node": "^13.13.52",
    "codelyzer": "^5.2.2",
    "hads": "^2.1.0",
    "htmlhint": "^0.11.0",
    "https-proxy-agent": "^5.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "^4.0.2",
    "karma-junit-reporter": "^2.0.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "phantomjs-prebuilt": "^2.1.16",
    "protractor": "~7.0.0",
    "stylelint": "~13.1.0",
    "stylelint-config-recommended-scss": "~4.2.0",
    "stylelint-config-standard": "~20.0.0",
    "stylelint-scss": "~3.14.2",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.0",
    "typescript": "4.0.2"
  }
}
